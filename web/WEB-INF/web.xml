<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>com.login.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.login.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Standing</servlet-name>
        <servlet-class>com.login.Standing</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SeasonPlayers</servlet-name>
        <servlet-class>com.actions.SeasonPlayers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashbord</servlet-name>
        <servlet-class>com.login.Dashbord</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetMatches</servlet-name>
        <servlet-class>com.login.GetMatches</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetFavourite</servlet-name>
        <servlet-class>com.login.GetFavourite</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddFav</servlet-name>
        <servlet-class>com.login.AddFav</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add</servlet-name>
        <servlet-class>com.actions.Add</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddMatch</servlet-name>
        <servlet-class>com.Admin.AddMatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteMatch</servlet-name>
        <servlet-class>com.Admin.DeleteMatch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Standing</servlet-name>
        <url-pattern>/Standing</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SeasonPlayers</servlet-name>
        <url-pattern>/SeasonPlayers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dashbord</servlet-name>
        <url-pattern>/Dashbord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetMatches</servlet-name>
        <url-pattern>/GetMatches</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetFavourite</servlet-name>
        <url-pattern>/GetFavourite</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddFav</servlet-name>
        <url-pattern>/AddFav</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add</servlet-name>
        <url-pattern>/Add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMatch</servlet-name>
        <url-pattern>/AddMatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteMatch</servlet-name>
        <url-pattern>/DeleteMatch</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
